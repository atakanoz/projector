#!/bin/bash

# Builder directory
BUILDERDIR=~/Desktop/projector
source "$BUILDERDIR/global.sh"



read -ep "Write the action type (starter | clone | import): " ACTION
[[ -z $ACTION ]] && exit || sleep 0


# If unknown answer
if [[ $ACTION != starter ]] && [[ $ACTION != clone ]] && [[ $ACTION != import ]]; then

	exit

fi